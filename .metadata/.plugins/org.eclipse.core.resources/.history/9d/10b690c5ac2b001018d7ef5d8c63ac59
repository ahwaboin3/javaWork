package c06.finalfield;

//final 필드와 상수
//final은 최종적이란 뜻을 가지고 있습니다.
public class FinalField {
	String nation="Korea";
	String name;
	
	public static void main(String[] args) {
		FinalField ff=new FinalField();
		System.out.println(ff.nation);
		ff.nation="Usa";
		System.out.println(ff.nation);
		System.out.println(ff.name);
		ff.name="김자바";
		System.out.println(ff.name);
	}
}







