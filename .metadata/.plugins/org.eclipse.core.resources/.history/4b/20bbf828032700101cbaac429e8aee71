package makeclass;

public class Movie {
	String title;
	String open;
	String runningTime;
	int price;
	
	public Movie(String title, String open, String runningTime, int price) {
		this.title = title;
		this.open = open;
		this.runningTime = runningTime;
		this.price = price;
	}

	String getField() {
		return "Movie [title=" + title + ", open=" 
				+ open + ", runningTime=" 
				+ runningTime + ", price=" 
				+ price + "]";
	}
	
	//관람 인원수에 따른 총 가격
	int getTotalPrice(int audience) {
		
	}
	
	
}






