package c07.friend;

public class FriendMain {
	
	public static void main(String[] args) {
		//친구 객체 생성
		Friend f1=new Friend();
		f1.setName("김친구1");
		Friend f2=new Friend();
		f2.setName("김친구2");
		Friend f3=new Friend();
		f3.setName("김친구3");
		
		//3명의 친구를 배열에 저장
		FriendRepo friendRepo=new FriendRepo();
		friendRepo.putFriend(f1);
		friendRepo.putFriend(f2);
		friendRepo.putFriend(f3);
		
		//배열에 저장된 내용 확인
		Friend[] friends=friendRepo.getFriends();
		for(int i=0;i<3;i++) {
			System.out.println(friends[i]);
		}
		
		
	}

}





