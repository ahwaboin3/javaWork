package c11.tast;

import java.util.Date;

public class Member {
	private String id;
	private String name;
	private Date date;
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public Date getDate() {
		return date;
	}
	public void setDate(Date date) {
		this.date = date;
	}
	@Override
	public String toString() {
		return "Member [id=" + id + ", name=" + name + ", date=" + date + "]";
	}
	
	public static void main(String[] args) {
		Member m1=new Member();
		m1.setId("id_java");
		m1.setName("김자바");
		m1.setDate(new Date());
	}

}
