package c06.ol;

//생성자 오버로딩
public class Car {
	String company;//필수
	String model;//필수
	String color;//필수
	int maxSpeed;
	
	public Car(String company, String model, 
			String color, int maxSpeed) {
		this.company = company;
		this.model = model;
		this.color = color;
		this.maxSpeed = maxSpeed;
	}

	public static void main(String[] args) {
		
	}

}




