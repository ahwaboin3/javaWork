package c07.instanceofdetails;

import c07.casting.Child;
import c07.casting.Parent;

//객체 타입 확인
public class InstanceofDetails {
	public static void main(String[] args) {
		Parent p1=new Parent();
		Parent p2=new Child();
		Parent[] pArr=new Parent[10];
		pArr[0]=p1;
		pArr[1]=p2;
		for(Parent p:pArr) {
			p.method1();
		}
		
	}
}
