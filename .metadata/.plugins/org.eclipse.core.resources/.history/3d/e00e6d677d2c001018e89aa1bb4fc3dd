package c07.autocast.book;

public class BookRepo {
	private Book[] books=new Book[100];
	
	public Book[] getBooks() {
		return books;
	}

	//book 객체 생성
	public Book getBook(String name) {
		Book book=new Book();
		book.setName(name);
		return book;
	}
	
	//배열에 book 객체 넣기
	public void addBook(Book book) {
		books[0]=book;
	}

}



