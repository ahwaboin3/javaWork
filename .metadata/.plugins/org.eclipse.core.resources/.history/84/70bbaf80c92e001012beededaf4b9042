package c07.friend;

public class FriendMain {
	
	public static void main(String[] args) {
		
		//3명의 친구 객체를 생성 및 배열에 저장
		FriendRepo friendRepo=new FriendRepo();
		friendRepo.putFriend(f1);
		friendRepo.putFriend(f2);
		friendRepo.putFriend(f3);
		
		//배열에 저장된 내용 확인
		Friend[] friends=friendRepo.getFriends();
		for(int i=0;i<3;i++) {
			System.out.println(friends[i]);
		}
		
		
	}

}





