package makeclass;

public class Movie {
	String title;
	String open;
	String runningTime;
	int price;
	
	public Movie(String title, String open, String runningTime, int price) {
		this.title = title;
		this.open = open;
		this.runningTime = runningTime;
		this.price = price;
	}

	public String toString() {
		return "Movie [title=" + title + ", open=" + open + ", runningTime=" + runningTime + ", price=" + price + "]";
	}
	
	
}
