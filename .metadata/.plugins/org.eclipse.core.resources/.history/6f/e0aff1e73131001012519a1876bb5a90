package c10.object;

public class Equals {
	public static void main(String[] args) {
		//java라는 아이디로 회원가입
		User[] users=new User[100];
		User user=new User();
		user.setId("java");
		users[0]=user;
		//로그인을 합니다
		String id=new String("java");
		if(users[0].getId()==id) {
			System.out.println("아이디가 일치합니다.");
		}else {
			System.out.println("아이디가 불일치합니다.");
		}
		
	}

}
