package makeclass;

public class Lotto {
	
	int[] nums;

	public Lotto(int[] nums) {
		this.nums = nums;
	}

	void getNums(){
		for(int n:nums) {
			System.out.println(n);
		}
	}
	
	public static void main(String[] args) {
		Lotto l1=new Lotto(new int[]{1,2,3,4,6});
		int[] nums= {2,3,4,5,6};
		Lotto l2=new Lotto(nums);
	}
	
}




