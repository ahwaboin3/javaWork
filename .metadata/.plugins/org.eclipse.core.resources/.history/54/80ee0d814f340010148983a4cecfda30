package c13.useenum;

public class SafeEnum {
	
	public void who(Person man) {
		switch(man) {
		case MAN:
			System.out.println("남성 손님입니다.");
			break;
		case WOMAN:
			System.out.println("여성 손님입니다.");
			break;
		}
	}
	public static void main(String[] args) {
		SafeEnum se=new SafeEnum();
		se.who(Person.MAN);
//		se.who(Animal.DOG);
	}
}
