package c10.object;

//기본 라이브러리(api, module)
//Object 클래스
//다른 클래스를 상속하지 않으면은 Object클래스를 상속하게 됩니다.
//자바의 모든 클래스는 Object클래스의 자식이거나 자손클래스입니다.
//Object는 자바의 최상위 부모 클래스에 해당합니다.
public class ObjectClass {
	public static void main(String[] args) {
		//객체 비교 (equals())
		//java라는 아이디로 회원가입
		User user=new User();
		user.setId("java");
		//로그인할 때 아이디 일치 여부 판단
		String id="java";
		if(user.getId()==id) {
			System.out.println("로그인 성공");
		}else {
			System.out.println("아이디 불일치");
		}
		
	}

}
